{
    "name": "symkit/error-bundle",
    "description": "Premium error page templates for Symfony applications",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": [
        "symfony",
        "bundle",
        "error",
        "templates",
        "error-pages"
    ],
    "version": "0.0.1",
    "authors": [
        {
            "name": "Symkit",
            "email": "s.dieunidou@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symkit\\ErrorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symkit\\ErrorBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/config": "^7.0 || ^8.0",
        "symfony/dependency-injection": "^7.0 || ^8.0",
        "symfony/framework-bundle": "^7.0 || ^8.0",
        "symfony/http-kernel": "^7.0 || ^8.0",
        "symfony/translation": "^7.0 || ^8.0",
        "symfony/twig-bundle": "^7.0 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.29",
        "nyholm/symfony-bundle-test": "^3.0",
        "phpro/grumphp": "^2.0",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.0",
        "deptrac/deptrac": "^2.0",
        "symfony/twig-bridge": "^7.0 || ^8.0",
        "symkit/bundle-ai-kit": "^0.0.1"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony": {
            "require": "^7.0 || ^8.0"
        },
        "bundle-ai-kit": {
            "editors": [
                "cursor"
            ],
            "skills": [
                "symfony-bundle-core",
                "symfony-bundle-ux",
                "symfony-bundle-doctrine",
                "symfony-bundle-flex",
                "symfony-bundle-quality"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "phpro/grumphp-shim": true,
            "phpro/grumphp": true,
            "symkit/bundle-ai-kit": true
        }
    }
}
